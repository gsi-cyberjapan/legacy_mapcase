			private function onLabelFontChange():void {
				var s:TextSymbolizer = this._editedSymbolize as TextSymbolizer;
				if(!s)
					return;
				if(!s.font)
					s.font = new org.openscales.core.style.font.Font();

				s.font.opacity = this._fontOpacity/100;
				s.font.color = this._fontColor;
				s.font.weight = this._fontBold?org.openscales.core.style.font.Font.BOLD:org.openscales.core.style.font.Font.NORMAL;
				s.font.style = this._fontItalic?org.openscales.core.style.font.Font.ITALIC:org.openscales.core.style.font.Font.NORMAL;
				s.font.family = this._fontFamily;
				s.font.size = this._fontSize;
				this.editStyleHandler.actualizeFeature();
			}
			
			private function onLabelHaloChange():void {
				var s:TextSymbolizer = this._editedSymbolize as TextSymbolizer;
				if(!s)
					return;
				if(this._haloRadius==0)
					s.halo=null;
				else {
					if(!s.halo)
						s.halo = new Halo();
					s.halo.radius = this._haloRadius;
					s.halo.color = this._haloColor;
				}
				
				this.editStyleHandler.actualizeFeature();
			}
			
