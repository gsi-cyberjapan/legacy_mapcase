			/**
			 *  Method to handle the left arrow checkbox on line style edition
			 */
			private function onLeftArrowChange():void
			{
				var am:ArrowMarker;
				if (this._leftArrowActivated)
				{
					var arrowSymb:ArrowSymbolizer = this._editedSymbolize as ArrowSymbolizer
					if ((this._editedSymbolize as LineSymbolizer).stroke.width < 3)
						am = new ArrowMarker(this._arrowMarkerType, null, (this._editedSymbolize as LineSymbolizer).stroke, 12);
					else
						am = new ArrowMarker(this._arrowMarkerType, null, (this._editedSymbolize as LineSymbolizer).stroke, (this._editedSymbolize as LineSymbolizer).stroke.width*4);
					
					
					if (this._editedSymbolize is ArrowSymbolizer)
					{
						(this._editedSymbolize as ArrowSymbolizer).leftMarker = am;
					}else{
						if ((this._editedSymbolize as LineSymbolizer).stroke.width < 3)
							am = new ArrowMarker(this._arrowMarkerType, null, (this._editedSymbolize as LineSymbolizer).stroke, 12);
						else
							am = new ArrowMarker(this._arrowMarkerType, null, (this._editedSymbolize as LineSymbolizer).stroke, (this._editedSymbolize as LineSymbolizer).stroke.width*4);
						
						arrowSymb = new ArrowSymbolizer((this._editedSymbolize as LineSymbolizer).stroke, am, null);
						this._editedSymbolize = arrowSymb;
						this.editStyleHandler.replaceSymbolizer(arrowSymb);
						this._isArrow = true;
					}
				}else{
					if (this._rightArrowActivated)
					{
						(this._editedSymbolize as ArrowSymbolizer).leftMarker = null;
					}else{
						var linSymb:LineSymbolizer = new LineSymbolizer((this._editedSymbolize as ArrowSymbolizer).stroke);
						this._editedSymbolize = linSymb;
						this.editStyleHandler.replaceSymbolizer(linSymb);
						this._isArrow = false;
					}
				}
				this.editStyleHandler.actualizeFeature();
			}
			
