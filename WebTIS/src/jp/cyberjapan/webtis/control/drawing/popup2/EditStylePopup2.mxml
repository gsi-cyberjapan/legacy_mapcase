			/**
			 * callback to handle line color change
			 */
			private function onLineColorChanged():void
			{
				var stroke:Stroke = (this._editedSymbolize as LineSymbolizer).stroke;
				stroke.color = this._lineColor;
				if (this._isArrow)
				{
					if ((this._editedSymbolize as ArrowSymbolizer).leftMarker)
					{
						((this._editedSymbolize as ArrowSymbolizer).leftMarker as ArrowMarker).stroke.color = this._lineColor;
					}
					if ((this._editedSymbolize as ArrowSymbolizer).rightMarker)
					{
						((this._editedSymbolize as ArrowSymbolizer).rightMarker as ArrowMarker).stroke.color = this._lineColor;
					}
				}
				this.editStyleHandler.actualizeFeature();
			}
			
			/**
			 * callback to handle line opacity change
			 */
			private function onLineOpacityChanged():void
			{
				var stroke:Stroke = (this._editedSymbolize as LineSymbolizer).stroke;
				stroke.opacity = this._lineBorderOpacity/100;
				if (this._isArrow)
				{
					if ((this._editedSymbolize as ArrowSymbolizer).leftMarker)
					{
						((this._editedSymbolize as ArrowSymbolizer).leftMarker as ArrowMarker).stroke.opacity = this._lineBorderOpacity/100;
					}
					if ((this._editedSymbolize as ArrowSymbolizer).rightMarker)
					{
						((this._editedSymbolize as ArrowSymbolizer).rightMarker as ArrowMarker).stroke.opacity = this._lineBorderOpacity/100;
					}
				}
				this.editStyleHandler.actualizeFeature();
			}
			
			/**
			 * callback to handle line width change
			 */
			private function onLineWidthChanged():void
			{
				var stroke:Stroke = (this._editedSymbolize as LineSymbolizer).stroke;
				stroke.width = int(this._lineWidth);
				if (this._isArrow)
				{
					var am:ArrowMarker;
					if ((this._editedSymbolize as ArrowSymbolizer).leftMarker)
					{
						((this._editedSymbolize as ArrowSymbolizer).leftMarker as ArrowMarker).stroke.width = int(this._lineWidth);
						am = ((this._editedSymbolize as ArrowSymbolizer).leftMarker as ArrowMarker);
						if (am.stroke.width < 3)
							am = new ArrowMarker(am.arrowMarker, am.fill, am.stroke, 12, am.opacity, am.rotation);
						else
							am = new ArrowMarker(am.arrowMarker, am.fill, am.stroke, am.stroke.width*4, am.opacity, am.rotation);
							
						(this._editedSymbolize as ArrowSymbolizer).leftMarker = am;
					}
					if ((this._editedSymbolize as ArrowSymbolizer).rightMarker)
					{
						((this._editedSymbolize as ArrowSymbolizer).rightMarker as ArrowMarker).stroke.width = int(this._lineWidth);
						am = ((this._editedSymbolize as ArrowSymbolizer).rightMarker as ArrowMarker);
						if (am.stroke.width < 3)
							am = new ArrowMarker(am.arrowMarker, am.fill, am.stroke, 12, am.opacity, am.rotation);
						else
							am = new ArrowMarker(am.arrowMarker, am.fill, am.stroke, am.stroke.width*4, am.opacity, am.rotation);
						
						(this._editedSymbolize as ArrowSymbolizer).rightMarker = am;
					}
				}
				this.editStyleHandler.actualizeFeature();
			}
			
